// Function composition example
use std.io

fn square(x: i64) -> i64
    x * x

fn double(x: i64) -> i64
    x + x

fn compose(x: i64) -> i64
    double(square(x))

fn main() -> i64 with [IO]
    print("Computing double(square(3))...")
    let result = compose(3)
    print("Result: 18 (returned as exit code)")
    result
