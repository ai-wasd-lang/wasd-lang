// Traits example - trait definitions and method implementations

trait Describable
    fn describe(self) -> i64

struct Point
    x: i64
    y: i64

impl Describable for Point
    fn describe(self) -> i64
        self.x + self.y

fn main() -> i64
    // Create a point
    let p = Point(x: 10, y: 20)

    // Call trait method - should return 30
    p.describe()
