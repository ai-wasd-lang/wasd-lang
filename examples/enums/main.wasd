// Enum and pattern matching examples

enum Option[T]
    Some(T)
    None

enum Result[T, E]
    Ok(T)
    Err(E)

fn main() -> i64
    // Create enum values
    let some_val = Option::Some(42)
    let none_val = Option::None()

    // Basic match
    let result = match some_val
        Some(_) => 1
        None => 0

    result
